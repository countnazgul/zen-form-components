{
	"name": "zen-form-components",
	"version": "0.0.1",
	"repository": "https://github.com/countnazgul/zen-form-components",
	"description": "Few Svelte form components",
	"author": "Stefan Stoichev / sstoichev.eu / stefan.stoichev@gmail.com / @countnazgul",
	"devDependencies": {
		"@babel/core": "7.10.5",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-transform-runtime": "7.10.5",
		"@babel/preset-env": "7.10.4",
		"@testing-library/cypress": "6.0.0",
		"autoprefixer": "9.8.5",
		"eslint": "7.5.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-cypress": "2.11.1",
		"eslint-plugin-svelte3": "2.7.3",
		"husky": "4.2.5",
		"lint-staged": "10.2.11",
		"node-sass": "4.14.1",
		"postcss": "8.2.10",
		"postcss-load-config": "2.1.0",
		"prettier": "2.0.5",
		"rollup": "2.22.2",
		"rollup-plugin-babel": "4.4.0",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-livereload": "1.3.0",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-svelte": "5.2.3",
		"rollup-plugin-terser": "6.1.0",
		"sirv-cli": "1.0.3",
		"start-server-and-test": "1.11.2",
		"svelte": "3.24.0",
		"svelte-preprocess": "4.0.8"
	},
	"files": [
		"src/components",
		"dist"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [
		"svelte"
	],
	"license": "MIT",
	"lint-staged": {
		"*.{html, css, scss, stylus, js, ts, json, yml, md}": [
			"prettier --write",
			"git add"
		],
		"*.{js, svelte}": [
			"eslint --fix",
			"git add"
		]
	},
	"main": "dist/index.min.js",
	"module": "dist/index.min.mjs",
	"scripts": {
		"build": "rollup -c",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"dev": "rollup -c -w",
		"lint": "eslint --color --ignore-path .gitignore .",
		"prepublishOnly": "npm run build",
		"start": "sirv public",
		"test": "start-server-and-test dev http://localhost:5000 cy:run"
	},
	"svelte": "src/components/components.module.js"
}